<Application
    x:Class="UWPXamlConverters.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:UWPXamlConverters.Converters"
    xmlns:local="using:UWPXamlConverters">

    <Application.Resources>
        <ResourceDictionary>
            <converters:StringtovisbilityConverter x:Name="StringToVisibilityConverter"></converters:StringtovisbilityConverter>
            <converters:StringToVisibilityReverseConverter x:Name="StringToVisibilityReverseConverter"></converters:StringToVisibilityReverseConverter>
            <converters:StringToBoolConverter x:Name="StringToBoolConverter"></converters:StringToBoolConverter>
            <converters:StringToBoolReverseConverter x:Name="StringToBoolReverseConverter"></converters:StringToBoolReverseConverter>
        </ResourceDictionary>

    </Application.Resources>
</Application>
